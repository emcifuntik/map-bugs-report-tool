{
  "compilerOptions": {
    "lib": [
      "esnext"
    ],
    "outDir": "./dist/server",
    "module": "ESNext",
    "incremental": true,
    "allowJs": false,
    "moduleResolution": "node",
    "target": "esnext",
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "removeComments": true,
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "preserveConstEnums": true,
    "sourceMap": true,
    "baseUrl": "./src",
    "rootDirs": [
      "src",
    ],
    "types": [
      "@altv/types-server",
      "node"
    ]
  }
}